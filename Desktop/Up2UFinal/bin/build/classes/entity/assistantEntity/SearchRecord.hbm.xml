<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity.assistantEntity">

<class name="SearchRecord" polymorphism="explicit">

	<id name="id">
		<generator class="increment" ></generator>
	</id>

	<property name="username"	type="string" 	column="username"  ></property>
	<property name="link" 		type="string"	column="link" 	   ></property>
	<property name="type" 		type="int" 		column="type" 	   ></property>
	<property name="tablename" 	type="string"	column="tablename" ></property>
	<property name="date" 		type="date"		column="date" 	   ></property>

</class>

</hibernate-mapping>